/*
 * Basic test keymap - just letters for debugging
 */

#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            label = "Test";
            bindings = <
                &kp A  &kp B  &kp C  &kp D  &kp E  &kp F    &kp G  &kp H  &kp I  &kp J  &kp K  &kp L
                &kp M  &kp N  &kp O  &kp P  &kp Q  &kp R    &kp S  &kp T  &kp U  &kp V  &kp W  &kp X
                &kp Y  &kp Z  &kp N1 &kp N2 &kp N3 &kp N4   &kp N5 &kp N6 &kp N7 &kp N8 &kp N9 &kp N0
                &kp TAB &kp ESC &kp SPACE &kp ENTER &kp BSPC &kp DEL  &kp HOME &kp END &kp PGUP &kp PGDN &kp LEFT &kp RIGHT
                       &kp LSHFT &kp LCTRL &kp LALT &kp LGUI &kp RGUI  &kp RALT &kp RCTRL &kp RSHFT &kp UP &kp DOWN
            >;
        };
    };
};